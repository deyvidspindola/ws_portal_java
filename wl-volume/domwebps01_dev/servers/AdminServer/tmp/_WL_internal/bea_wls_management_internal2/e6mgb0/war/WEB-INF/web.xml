<?xml version="1.0" ?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <servlet>
      <servlet-name>Bootstrap</servlet-name>
      <servlet-class>weblogic.management.servlet.BootstrapServlet</servlet-class>
    </servlet>

    <servlet>
      <servlet-name>FileDistribution</servlet-name>
      <servlet-class>weblogic.management.servlet.FileDistributionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
      <servlet-name>FileDistribution</servlet-name>
      <url-pattern>/wl_management</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
      <servlet-name>Bootstrap</servlet-name>
      <url-pattern>/Bootstrap</url-pattern>
    </servlet-mapping>
</web-app>